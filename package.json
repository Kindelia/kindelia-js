{
  "name": "kindelia-js",
  "version": "1.0.0",
  "dependencies": {
    "axios": "1.1.2",
    "rustie": "0.2.0"
  },
  "devDependencies": {
    "@types/jest": "29.1.2",
    "jest": "29.2.0",
    "rome": "10.0.1",
    "ts-jest": "29.0.3",
    "tsup": "6.2.3",
    "typescript": "4.8.4"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "repository": "git@github.com:Kindelia/kindelia-js.git",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --watch --format cjs,esm --dts --sourcemap",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:all": "jest --coverage",
